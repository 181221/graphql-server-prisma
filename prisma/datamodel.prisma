type Movie {
  id: ID! @id
  createdAt: DateTime! @createdAt
  title: String!
  requestedBy: User @relation(link: INLINE)
  img: String
  tmdb_id: String
  genres: [String]
  release_date: String
  vote_average: String
  overview: String
}

type User { 
  id: ID! @id
  name: String
  email: String! @unique
  movies: [Movie!]! 
} 

